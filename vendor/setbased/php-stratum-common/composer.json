{
  "name": "setbased/php-stratum-common",
  "description": "PhpStratum Common",
  "keywords": [
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.1",
    "ext-mbstring": "*",
    "setbased/exception": "^2.3.0",
    "setbased/helper-cast": "^3.0.1",
    "setbased/helper-code-store-php": "^2.6.0",
    "setbased/php-stratum-backend": "^6.1.0",
    "setbased/php-stratum-middle": "^5.12.0"
  },
  "require-dev": {
    "phing/phing": "^3.0.0",
    "phpunit/phpunit": "^10.5.35"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Stratum\\Common\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SetBased\\Stratum\\Common\\Test\\": "test/"
    }
  },
  "config": {
    "bin-dir": "bin/",
    "sort-packages": true,
    "allow-plugins": {
      "phing/phing-composer-configurator": true
    }
  }
}
